{
  "name": "nwb-react-lib-test",
  "version": "1.0.0",
  "description": "nwb-react-lib-test React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "sass",
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "clean-css": "rimraf css",
    "build-css": "npm run clean-css && node-sass-chokidar sass/ -o css/",
    "watch-css": "npm run build-css && node-sass-chokidar sass/ -o css/ --watch --recursive",
    "build-js": "nwb build-react-component",
    "start-js": "nwb serve-react-demo",
    "build": "npm-run-all build-css build-js",
    "start": "npm-run-all watch-css start-js",
    "clean": "npm run clean-css && nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "nwb": "^0.23.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/jest.cssStub.js"
    }
  }
}
